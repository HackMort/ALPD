        /**
         * #################################################################################################
         * CaseFileModal
         * #################################################################################################
         */

        import { Fancybox } from "@fancyapps/ui";

        class caseFileModalDesktop {
            caseFileModalDesktop: Fancybox;

            constructor() {
                this.caseFileModalDesktop = undefined;
            }

            open() {
                this.caseFileModalDesktop = new Fancybox(
                    [{ src: "case-files-modal--desktop" }],
                    {
                        autoFocus: true,
                        defaultType: "inline",
                        placeFocusBack: false,
                        trapFocus: false,
                        closeButton: false,
                        height: "auto",
                        mainClass: "case-files-modal--destkop",
                        dragToClose: false,
                        on: {
                            reveal: this.goToTab,
                            destroy: this.resetTabs
                        }
                    }
                );
            }

            goToTab() {
                const caseFileDesktopModal = document.querySelector('#case-files-modal-template-desktop')
                const dataActiveTab = caseFileDesktopModal.dataset.activeTab
                const newActiveTab = document.querySelector(`#case-link-to-panel-${dataActiveTab}`)
                if(newActiveTab) {
                    newActiveTab.click()
                }
            }

            resetTabs() {
                const caseFileDesktopModal = document.querySelector('#case-files-modal-template-desktop')
                caseFileDesktopModal.dataset.activeTab = 1
            }

            close() {
                this.caseFileModalDesktop.destroy();
            }
        }

        document.addEventListener("DOMContentLoaded", () => {
            const openCaseFilesModal = new caseFileModalDesktop();
            const buttons = document.querySelectorAll(
                ".case-files-modal-trigger--desktop"
            );
            const customTabLinks = document.querySelectorAll("[data-tab-id]");

            buttons.forEach((button) => {
                button.addEventListener("click", (event) => {
                    event.preventDefault();
                    const caseFileDesktopModal = document.querySelector('#case-files-modal-template-desktop')
                    let dataTab = event.target.dataset.caseFileTab
                    if(dataTab) {
                        caseFileDesktopModal.dataset.activeTab = dataTab
                    }
                    openCaseFilesModal.open();

                    const mainTablist: HTMLListElement = document.querySelector(
                        '#case-files-modal--desktop .case-files__main-tabs[role="tablist"]'
                    );
                    const tabs = mainTablist.querySelectorAll("a");

                    tabs.forEach((tab) => {

                        if (tab.ariaSelected) {
                            mainTablist.dataset.activeTab = tab.id;
                        }

                        tab.addEventListener("click", () => {
                            mainTablist.dataset.activeTab = tab.id;
                        });
                    });
                });
            });

            customTabLinks.forEach((link) => {
                link.addEventListener("click", (e) => {
                    e.preventDefault();
                    const tabLink = document.querySelector(
                        `a[href="#${link.getAttribute("data-tab-id")}"]`
                    ) as HTMLAnchorElement;
                    tabLink.click();
                });
            });
        });
    </script>
