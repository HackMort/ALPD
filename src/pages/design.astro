      <table>
        <thead>
          <tr>
            <th>Tag</th>
            <th>Weight</th>
            <th>Size</th>
            <th>Line height</th>
          </tr>
        </thead>
        <tbody>
          <tr class="h1">
            <td style="font-family: veneer;">h1</td>
            <td style="font-family: veneer;">Bold</td>
            <td style="font-family: veneer;">40px</td>
            <td style="font-family: veneer;">46.88px</td>
          </tr>
          <tr class="h2">
            <td style="font-family: veneer;">h2</td>
            <td style="font-family: veneer;">600</td>
            <td style="font-family: veneer;">30px</td>
            <td style="font-family: veneer;">35.16px</td>
          </tr>
          <tr class="h3">
            <td style="font-family: veneer;">h3</td>
            <td style="font-family: veneer;">500</td>
            <td style="font-family: veneer;">24px</td>
            <td style="font-family: veneer;">28.13px</td>
          </tr>
          <tr class="h4">
            <td style="font-family: veneer;">h4</td>
            <td style="font-family: veneer;">500</td>
            <td style="font-family: veneer;">20px</td>
            <td style="font-family: veneer;">23.44px</td>
          </tr>
          <tr class="h5">
            <td style="font-family: veneer;">h5</td>
            <td style="font-family: veneer;">500</td>
            <td style="font-family: veneer;">16px</td>
            <td style="font-family: veneer;">24px</td>
          </tr>
          <tr class="p">
            <td style="font-family: veneer;">p</td>
            <td style="font-family: veneer;">400</td>
            <td style="font-family: veneer;">18px</td>
            <td style="font-family: veneer;">21.09px</td>
          </tr>
          <tr class="small">
            <td style="font-family: veneer;">small</td>
            <td style="font-family: veneer;">300</td>
            <td style="font-family: veneer;">12px</td>
            <td style="font-family: veneer;">14.06px</td>
          </tr>
        </tbody>
      </table>
    </div>
  </section>
  <section class="design-system-section">
    <div class="section-wrapper section-wrapper--small">
      <h1 class="title">Buttons</h1>
      <div class="buttons">
        <!-- Default button -->
        <button class="button"> Button Primary / Default</button>
        <button class="button button--secondary"> Button Secondary</button>
        <button class="button button--take-photo"> Button take-photo</button>
        <button class="button button--upload-photo">
          Button upload-photo</button
        >
        <button class="button button--tertiary"> Tertiary Button</button>
      </div>
    </div>
  </section>
  <section class="design-system-section">
    <div class="section-wrapper section-wrapper--small">
      <h1 class="title">Red string</h1>
      <div class="red-string"></div>
      <div class="elements">
        <img
          class="fingerprint"
          src="/assets/images/footer-finger-print-2x.png"
          alt=""
        />
        <img class="red-frame" src="/assets/images/red-frame.png" alt="" />
